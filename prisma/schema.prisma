generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x", "rhel-openssl-3.0.x"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Trivia {
  id           String   @id @default(cuid())
  question     String
  choices      String[]
  answer_index Int
  created_at   DateTime @default(now())
}
